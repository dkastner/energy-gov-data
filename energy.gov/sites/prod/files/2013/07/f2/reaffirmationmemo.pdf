<script language="JavaScript">

function setBase()
  {
  var bref = "http://www.hss.doe.gov/";
  var currentLocation = window.location.href.toLowerCase();
  var beta = currentLocation.indexOf('/beta/template/');
  var practice = currentLocation.indexOf('/practice/');
  var accountreq = currentLocation.indexOf('/accountrequest/');
  //var hssnew = currentLocation.indexOf('/hssnew/');
 

  if(beta > -1)
  {
     bref = "http://www.hss.doe.gov/beta/template/";
  }
  else if(practice > -1)
  {
     bref = "http://www.hss.doe.gov/practice/";
